#include <iostream>
using namespace std;
/* run this program using the console pauser or add your own getch, system("pause") or input loop */

int main(int argc, char** argv) {
	
	int n;
	int aux; 
	
	cout<<"ingrese una cantidad de numeros:" ; 
	cin>>n;
	int numero[n];
	
	for(int i=0;i<=n;i++){
	cout<<"Ingrese un numero: ";
	cin>>numero[i];
	}
	for(int i=0;i<=n;i++){
	cout<<"["<<i<<"]"<<numero[i]<<endl;
	}

	for(int k=0;k<=n;k++){
		for(int j=0;j<=n;j++){
			if(numero[j]>numero[k]){
				aux=numero[j];
				numero[j]=numero[k];
				numero[k]=aux;
			}
		}
	}
	for(int i=0;i<=n;i++){
	cout<<numero[i]<<endl;
	}
	return 0;
}

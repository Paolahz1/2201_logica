#include <iostream>

using namespace std;

int main(){

 
 int num[4];
 int n,j,R,i;
 
  for(i=0;i<4;i++){
       do{
            cout<<"Ingrese un nmr: ";
            cin>>n;
            j=1;
            R=0;
            while(j<i||R==0){
                if(num[i]==n){
                 R=1;
                }
                j+=1;
            }
      }while(R==1);
      
     num[i]=n;
  }

    return 0;
}
